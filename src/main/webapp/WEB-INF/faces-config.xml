<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
	version="2.2">

	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver
		</el-resolver>
		<action-listener>
			org.primefaces.application.DialogActionListener
		</action-listener>
		<navigation-handler>
			org.primefaces.application.DialogNavigationHandler
		</navigation-handler>
		<view-handler>
			org.primefaces.application.DialogViewHandler
		</view-handler>
	</application>
	<navigation-rule>
		<from-view-id>/*</from-view-id>
		<navigation-case>
			<from-outcome>listgames</from-outcome>
			<to-view-id>/ui/list/listGames.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>listgenre</from-outcome>
			<to-view-id>/ui/list/listGenre.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>listinterview</from-outcome>
			<to-view-id>/ui/list/listInterviewStage.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>liststatus</from-outcome>
			<to-view-id>/ui/list/listStatus.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>listworkmode</from-outcome>
			<to-view-id>/ui/list/listWorkMode.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>listempl</from-outcome>
			<to-view-id>/ui/list/listEmployes.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
			<navigation-case>
			<from-outcome>listcandidates</from-outcome>
			<to-view-id>/ui/list/listCandidates.xhtml</to-view-id>
			<redirect/>
	    </navigation-case>
		<navigation-case>
			<from-outcome>addGame</from-outcome>
			<to-view-id>/ui/list/addGame.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>addGenre</from-outcome>
			<to-view-id>/ui/list/addGenre.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
		<navigation-case>
			<from-outcome>addInterview_Stage</from-outcome>
			<to-view-id>/ui/list/addInterview_Stage.xhtml</to-view-id>
			<redirect/>
		</navigation-case>
	</navigation-rule>

</faces-config>